<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="1dip" android:color="#002008" />
            <gradient android:angle="-90" android:startColor="#012804" android:endColor="#012309"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="1dip" android:color="#f7e3e3" />
            <solid android:color="#61c645"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="1dip" android:color="#01280c" />
            <gradient android:angle="-90" android:startColor="#ffffff" android:endColor="#fffefe" />
        </shape>
    </item>
</selector>