<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="1dip" android:color="#f7e3e3" />
            <solid android:color="#1AE90B"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="2dip" android:color="#FDFCFC" />
            <gradient
                android:centerColor="#f7f304"
                android:endColor="#f9fdce"
                android:gradientRadius="400"
                android:startColor="#F8B103"
                android:angle="90" >
            </gradient>
        </shape>
    </item>
</selector>